<h1>Users::Rooms#index</h1>
<p>Find me in app/views/users/rooms/index.html.erb</p>
<div class="container">
  <div class="row">
    <table class="table">
      <thead>
      <thead>
        <tr>
          <th>メッセージ相手</th>
          <th>メッセージ内容</th>
        </tr>
      </thead>
      </thead>
      <tbody>
        <% @another_entries.each do |entry|%>
          <tr>
            <td>
              <%= link_to entry.user.name, user_path(entry.user) %>
            </td>
            <td>
              <%= link_to Message.find_by(id: entry.room.message_ids.last)&.body, room_path(entry.room) %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>